{
    "name": "deko96/auth-gg-laravel",
    "description": "Simple Auth.gg wrapper for Laravel",
    "keywords": [
        "licenses",
        "users",
        "auth.gg",
        "auth",
        "hwid"
    ],
    "homepage": "https://github.com/deko96/auth-gg",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dejan Bozhinoski",
            "email": "bozhinoski996@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3",
        "illuminate/support": "^8.0.0 || ^9.0.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Deko96\\AuthGG\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deko96\\AuthGG\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deko96\\AuthGG\\AuthGGServiceProvider"
            ],
            "aliases": {
                "AuthGG": "Deko96\\AuthGG\\AuthGGFacade"
            }
        }
    }
}
